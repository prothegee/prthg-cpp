#ifndef PRTHGCPP_CONFIG_H
#define PRTHGCPP_CONFIG_H


#ifndef __PRTHGCPP_MAJOR__
    // prthgcpp major version
    #define __PRTHGCPP_MAJOR__ @PRTHGCPP_MAJOR@
#endif // __PRTHGCPP_MAJOR__

#ifndef __PRTHGCPP_MINOR__
    // prthgcpp minor version
    #define __PRTHGCPP_MINOR__ @PRTHGCPP_MINOR@
#endif // __PRTHGCPP_MINOR__

